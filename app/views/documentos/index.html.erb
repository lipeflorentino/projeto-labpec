<p id="notice"><%= notice %></p>

<h1>Documentos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Titulo</th>
      <th>Descricao</th>
      <th>Status</th>
      <th>Data defesa</th>
      <th>Tese</th>
      <th>Mestrado</th>
      <th>Download</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @documentos.each do |documento| %>
      <tr>
        <td><%= documento.user_id %></td>
        <td><%= documento.titulo %></td>
        <td><%= documento.descricao %></td>
        <td><%= documento.status %></td>
        <td><%= documento.data_defesa %></td>
        <td><%= documento.tese %></td>
        <td><%= documento.mestrado %></td>
        <td><%= link_to "Download", documento.arquivo_url %></td>
        <td><%= link_to 'Show', documento %></td>
        <td><%= link_to 'Edit', edit_documento_path(documento) %></td>
        <td><%= link_to 'Destroy', documento, method: :delete, data: { confirm: 'VocÃª teem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Documento', new_documento_path %>
