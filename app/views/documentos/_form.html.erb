<%= form_for(documento) do |f| %>
  <% if documento.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(documento.errors.count, "error") %> prohibited this documento from being saved:</h2>

      <ul>
      <% documento.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :user_id %>
    <%= f.number_field :user_id %>
  </div>

  <div class="field">
    <%= f.label :titulo %>
    <%= f.text_field :titulo %>
  </div>

  <div class="field">
    <%= f.label :descricao %>
    <%= f.text_area :descricao %>
  </div>

  <div class="field">
    <%= f.label :status %>
    <%= f.check_box :status %>
  </div>

  <div class="field">
    <%= f.label :data_defesa %>
    <%= f.date_select :data_defesa %>
  </div>

  <div class="field">
    <%= f.label :tese %>
    <%= f.check_box :tese %>
  </div>

  <div class="field">
    <%= f.label :mestrado %>
    <%= f.check_box :mestrado %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
